    "nyc": "^12.0.2",
    "prettier": "1.13.7",
    "tape": "^4.9.1"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/eslint-config-pretty-strict/index.js"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "root": true
  },
  "esm": {
    "cjs": true,
